{
  "name": "@chernodub/eslint-config",
  "version": "4.0.2",
  "description": "typescript-only, framework-agnostic eslint config",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chernodub/eslint-config"
  },
  "main": "index.js",
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "tslint"
  ],
  "author": "Viktor Chernodub",
  "license": "MIT",
  "scripts": {
    "release": "release-it"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.8.0"
  },
  "devDependencies": {
    "release-it": "*"
  },
  "release-it": {
    "github": {
      "release": false
    },
    "git": {
      "tag": false,
      "commitMessage": "chore: release ${version}"
    },
    "npm": {
      "publish": false
    }
  }
}
